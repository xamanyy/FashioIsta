<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fashionista">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fashionistaicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/fashionistaicon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fashionista">
        <activity
            android:name=".mens"
            android:exported="true"
            android:label="@string/title_activity_mens"
            android:theme="@style/Theme.Fashionista.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Fashionista.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".Home"/>
        <activity android:name=".LoginPage"/>
        <activity android:name=".Men.MenCloth"/>
        <activity android:name=".Men.MenFormal"/>
        <activity android:name=".Men.MenCasual"/>
        <activity android:name=".Men.MenTopCasualActivity"/>
        <activity android:name=".Women.WomenCloth"/>
        <activity android:name=".Women.WomenFormal"/>
        <activity android:name=".Women.WomenCasual"/>

    </application>


</manifest>